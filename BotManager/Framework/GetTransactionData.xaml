  <Sequence sap2010:Annotation.AnnotationText="Description: Get data from UiPath server queue and add to Local Database. If no new data, set out_TransactionItem to Nothing. &#xA;" DisplayName="GetTransactionData and Add to Local Queue" sap2010:WorkflowViewState.IdRef="Sequence_1">
